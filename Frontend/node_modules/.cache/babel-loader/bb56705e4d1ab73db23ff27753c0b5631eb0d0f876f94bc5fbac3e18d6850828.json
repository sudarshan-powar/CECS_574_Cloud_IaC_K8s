{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nconst axios = require('axios');\nexport default {\n  data() {\n    return {\n      token: null,\n      orderList: []\n    };\n  },\n  props: [\"baseURL\"],\n  name: 'Order',\n  methods: {\n    // list of order histories\n    listOrders() {\n      axios.get(`${this.baseURL}order/?token=${this.token}`).then(response => {\n        if (response.status == 200) {\n          this.orders = response.data;\n          // for each order populate orderList\n          this.orders.forEach(order => {\n            this.orderList.push({\n              id: order.id,\n              totalCost: order.totalPrice,\n              // get short date\n              orderdate: order.createdDate.substring(0, 10),\n              // get image of the first orderItem of the order\n              imageURL: order.orderItems[0].product.imageURL,\n              // get total items\n              totalItems: order.orderItems.length\n            });\n          });\n        }\n      }, error => {\n        console.log(error);\n      });\n    }\n  },\n  mounted() {\n    this.token = localStorage.getItem(\"token\");\n    this.listOrders();\n  }\n};","map":{"version":3,"names":["axios","require","data","token","orderList","props","name","methods","listOrders","get","baseURL","then","response","status","orders","forEach","order","push","id","totalCost","totalPrice","orderdate","createdDate","substring","imageURL","orderItems","product","totalItems","length","error","console","log","mounted","localStorage","getItem"],"sources":["/Users/sudarshanpowar/Desktop/Personal/project2/Ecomm-UI/ecommerce-vuejs/src/views/Orders/Order.vue"],"sourcesContent":["<template>\r\n  <div class=\"container\">\r\n    <div class=\"row\">\r\n      <div class=\"col-12 text-center\">\r\n        <h4 class=\"pt-3\">Your Orders</h4>\r\n      </div>\r\n    </div>\r\n    <!--        for each order display -->\r\n    <div v-for=\"order in orderList\" :key=\"order.pid\" class=\"row mt-2 pt-3 justify-content-around\">\r\n      <div class=\"col-2\"></div>\r\n      <div class=\"col-md-3 embed-responsive embed-responsive-16by9\">\r\n        <!--                display image in left-->\r\n        <img v-bind:src=\"order.imageURL\" class=\"w-100 card-img-top embed-responsive-item\">\r\n      </div>\r\n      <div class=\"col-md-5 px-3\">\r\n        <div class=\"card-block px-3\">\r\n          <h6 class=\"card-title\">\r\n            <router-link v-bind:to=\"'/order/'+order.id\">Order No : {{order.id}}</router-link>\r\n          </h6>\r\n          <p class=\"mb-0\">{{order.totalItems}} item<span v-if=\"order.totalItems > 1\">s</span></p>\r\n          <p id=\"item-price\" class=\"mb-0 font-weight-bold\">Total Cost : $ {{order.totalCost}}</p>\r\n          <p id=\"item-total-price\">Ordered on : {{order.orderdate}}</p>\r\n        </div>\r\n      </div>\r\n      <div class=\"col-2\"></div>\r\n      <div class=\"col-12\"><hr></div>\r\n    </div>\r\n  </div>\r\n\r\n</template>\r\n\r\n<script>\r\n  const axios = require('axios')\r\n  export default {\r\n\r\n    data() {\r\n      return {\r\n        token: null,\r\n        orderList : []\r\n      }\r\n    },\r\n    props:[\"baseURL\"],\r\n    name: 'Order',\r\n    methods: {\r\n      // list of order histories\r\n      listOrders(){\r\n        axios.get(`${this.baseURL}order/?token=${this.token}`)\r\n          .then((response) => {\r\n              if(response.status==200){\r\n                this.orders = response.data\r\n                // for each order populate orderList\r\n                this.orders.forEach((order) => {\r\n                  this.orderList.push({\r\n                    id: order.id,\r\n                    totalCost: order.totalPrice,\r\n                    // get short date\r\n                    orderdate: order.createdDate.substring(0,10),\r\n                    // get image of the first orderItem of the order\r\n                    imageURL: order.orderItems[0].product.imageURL,\r\n                    // get total items\r\n                    totalItems: order.orderItems.length\r\n                  })\r\n                })\r\n              }\r\n            },\r\n            (error)=>{\r\n              console.log(error)\r\n            });\r\n      },\r\n    },\r\n    mounted() {\r\n      this.token = localStorage.getItem(\"token\");\r\n      this.listOrders();\r\n    },\r\n  };\r\n\r\n</script>\r\n\r\n<style scoped>\r\n  h4, h5 {\r\n    font-family: 'Roboto', sans-serif;\r\n    color: #484848;\r\n    font-weight: 700;\r\n  }\r\n\r\n  .embed-responsive .card-img-top {\r\n    object-fit: cover;\r\n  }\r\n</style>\r\n"],"mappings":";AAgCE,MAAMA,KAAI,GAAIC,OAAO,CAAC,OAAO;AAC7B,eAAe;EAEbC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,KAAK,EAAE,IAAI;MACXC,SAAQ,EAAI;IACd;EACF,CAAC;EACDC,KAAK,EAAC,CAAC,SAAS,CAAC;EACjBC,IAAI,EAAE,OAAO;EACbC,OAAO,EAAE;IACP;IACAC,UAAUA,CAAA,EAAE;MACVR,KAAK,CAACS,GAAG,CAAE,GAAE,IAAI,CAACC,OAAQ,gBAAe,IAAI,CAACP,KAAM,EAAC,EAClDQ,IAAI,CAAEC,QAAQ,IAAK;QAChB,IAAGA,QAAQ,CAACC,MAAM,IAAE,GAAG,EAAC;UACtB,IAAI,CAACC,MAAK,GAAIF,QAAQ,CAACV,IAAG;UAC1B;UACA,IAAI,CAACY,MAAM,CAACC,OAAO,CAAEC,KAAK,IAAK;YAC7B,IAAI,CAACZ,SAAS,CAACa,IAAI,CAAC;cAClBC,EAAE,EAAEF,KAAK,CAACE,EAAE;cACZC,SAAS,EAAEH,KAAK,CAACI,UAAU;cAC3B;cACAC,SAAS,EAAEL,KAAK,CAACM,WAAW,CAACC,SAAS,CAAC,CAAC,EAAC,EAAE,CAAC;cAC5C;cACAC,QAAQ,EAAER,KAAK,CAACS,UAAU,CAAC,CAAC,CAAC,CAACC,OAAO,CAACF,QAAQ;cAC9C;cACAG,UAAU,EAAEX,KAAK,CAACS,UAAU,CAACG;YAC/B,CAAC;UACH,CAAC;QACH;MACF,CAAC,EACAC,KAAK,IAAG;QACPC,OAAO,CAACC,GAAG,CAACF,KAAK;MACnB,CAAC,CAAC;IACR;EACF,CAAC;EACDG,OAAOA,CAAA,EAAG;IACR,IAAI,CAAC7B,KAAI,GAAI8B,YAAY,CAACC,OAAO,CAAC,OAAO,CAAC;IAC1C,IAAI,CAAC1B,UAAU,CAAC,CAAC;EACnB;AACF,CAAC"},"metadata":{},"sourceType":"module","externalDependencies":[]}